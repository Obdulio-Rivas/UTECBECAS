                                    <span class="etiqueta">El aplicante indico, que no ha realizado servicio social aun.</span>
                                </div>
                            </div>
                        <%}%>
                    </div>
                    <div class="controles-view">
                        <a href="#" id="back_button">Regresar</a>
                        <%if(data_user.Nivel === 2){%>
                            <%if (filas[0].Estado[1] === 1) {%>
                                <a id="deny-view" class="deny-view" href="applicant_view_action?id=<%- filas[0].IDformulario %>,action=deny"><i id="<%- filas[0].IDformulario %>"></i>Denegar</a>
                                <a id="approve-view" class="approve-view" href="applicant_view_action?id=<%- filas[0].IDformulario %>,action=approve"><i id="<%- filas[0].IDformulario %>"></i>Aprobar</a>
                            <%}else if (filas[0].Estado[1] === 2) {%>
                                <a id="deny-view" class="deny-view" href="applicant_view_action?id=<%- filas[0].IDformulario %>,action=deny"><i id="<%- filas[0].IDformulario %>"></i>Denegar</a>
                            <%} else {%>
                                <a id="approve-view" class="approve-view" href="applicant_view_action?id=<%- filas[0].IDformulario %>,action=approve"><i id="<%- filas[0].IDformulario %>"></i>Aprobar</a>
                            <%}%>
                        <%}%>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>