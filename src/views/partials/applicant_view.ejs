                                    <%
                                    var tiempo_t = filas[0].Tiempo_de_trabajo_empresa;
                                    if(tiempo_t ==="1") { %>
                                        <span class="info-etiqueta">Menos de un Año</span>
                                    <% }else  if(tiempo_t ==='2'){ %>
