                                <div class="block-2">
                                    <span class="etiqueta">Numero de Telefono:</span>
                                    <span class="info-etiqueta"><%- filas[0].Telefono_Trabajo %> </span>
                                </div>
                            </div>
                            <div class="blocks-row">
                                <div class="block-2">
                                    <span class="etiqueta">Cargo:</span>
                                    <span class="info-etiqueta"><%- filas[0].Cargo_que_desempena %> </span>
                                </div>
                                <div class="block-2">
                                    <span class="etiqueta">Salario Actual:</span>
                                    <%
                                    var Salario_actual = filas[0].Salario_actual;
                                    if(Salario_actual ===1) { %>
                                        <span class="info-etiqueta">Menos de $100 Mensuales.</span>
                                    <% }else  if(Salario_actual ===2){ %>
                                        <span class="info-etiqueta">Mas de $100 Mensuales.</span>
                                    <% }else  if(Salario_actual ===3){ %>
                                        <span class="info-etiqueta">Menos de $200 Mensuales.</span>
                                    <% }else  if(Salario_actual ===4){ %>
                                        <span class="info-etiqueta">Mas de $200 Mensuales.</span>
                                    <% }else  if(Salario_actual ===5){ %>
                                        <span class="info-etiqueta">Menos de $300 Mensuales.</span>
                                    <% }else  if(Salario_actual ===6){ %>
                                        <span class="info-etiqueta">Mas de $300 Mensuales.</span>
                                    <% }else  if(Salario_actual ===7){ %>
                                        <span class="info-etiqueta">Menos de $400 Mensuales.</span>
                                    <% }else  if(Salario_actual ===8){ %>
                                        <span class="info-etiqueta">Mas de $400 Mensuales.</span>
                                    <% }else  if(Salario_actual ===9){ %>
                                        <span class="info-etiqueta">Menos de $500 Mensuales.</span>
                                    <% }else  if(Salario_actual ===10){ %>
                                        <span class="info-etiqueta">Mas de $500 Mensuales.</span>
                                    <% }else  if(Salario_actual ===11){ %>
                                        <span class="info-etiqueta">Menos de $600 Mensuales.</span>
                                    <% }else  if(Salario_actual ===12){ %>
                                        <span class="info-etiqueta">Mas de $600 Mensuales.</span>
                                    <% }else  if(Salario_actual ===13){ %>
                                        <span class="info-etiqueta">Menos de $700 Mensuales.</span>
                                    <% }else  if(Salario_actual ===14){ %>
                                        <span class="info-etiqueta">Mas de $700 Mensuales.</span>
                                    <% }else  if(Salario_actual ===15){ %>
                                        <span class="info-etiqueta">Menos de $800 Mensuales.</span>
                                    <% }else  if(Salario_actual ===16){ %>
                                        <span class="info-etiqueta">Mas de $800 Mensuales.</span>
                                    <% }else  if(Salario_actual ===17){ %>
                                        <span class="info-etiqueta">Menos de $900 Mensuales.</span>
                                    <% }else  if(Salario_actual ===18){ %>
                                        <span class="info-etiqueta">Mas de $900 Mensuales.</span>
                                    <% }else  if(Salario_actual ===19){ %>
                                        <span class="info-etiqueta">Menos de $1000 Mensuales.</span>
                                    <% }else  if(Salario_actual ===20){ %>
                                        <span class="info-etiqueta">Mas de $1000 Mensuales.</span>
                                    <% }else{ %>
                                        <span class="info-etiqueta">No Especifico.</span>
                                    <% } %>
                                </div>
                            </div>
                            <div class="blocks-row">
                                <div class="block-2">
                                    <span class="etiqueta">Nombre del Jefe:</span>
                                    <span class="info-etiqueta"><%- filas[0].Nombre_jefe %> </span>
                                </div>
                                <div class="block-2">
                                    <span class="etiqueta">Telefono Jefe:</span>
                                    <span class="info-etiqueta"><%- filas[0].Telefono_jefe %> </span>
                                </div>
                            </div>
                        <% }else{ %>
                                <div class="blocks-row">
                                    <div class="block-2">
                                        <span class="etiqueta">Aplicante indico que no Trabaja.</span>
                                    </div>
                                </div>
                        <% } %>
                    </div>
                    <div id="content_block_part_4" class="block-content block-content-none">
                        <span class="title-content">Estudios Realizados.</span>
                        <span class="divider-line"></span>
                        <div class="blocks-row">
                            <div class="block-2">
                                <span class="etiqueta">Educacion Basico:</span>
                                <span class="info-etiqueta"><%- filas[0].Educacion_Basico %> </span>
                            </div>
                            <div class="block-2">
                                <span class="etiqueta">Bachillerato:</span>
                                <span class="info-etiqueta"><%- filas[0].Bachillerato %> </span>
                            </div>
                        </div>
                        <div class="blocks-row">
                            <div class="block-2">
                                <span class="etiqueta">Tecnico:</span>
                                <span class="info-etiqueta"><%- filas[0].Tecnico %> </span>
                            </div>
                            <div class="block-2">
                                <span class="etiqueta">Grado Universitario:</span>
                                <span class="info-etiqueta"><%- filas[0].Grado_Universitario %> </span>
                            </div>
                        </div>
                        <div class="blocks-row">
                            <div class="block-2">
                                <span class="etiqueta">Post Grado:</span>
                                <span class="info-etiqueta"><%- filas[0].Post_grado %> </span>
                            </div>
                            <div class="block-2">
                                <span class="etiqueta">Maestria:</span>
                                <span class="info-etiqueta"><%- filas[0].Maestria %> </span>
                            </div>
                        </div>
                        <div class="blocks-row">
                            <div class="block-2">
                                <span class="etiqueta">Post Grado:</span>
                                <span class="info-etiqueta"><%- filas[0].Post_grado %> </span>
                            </div>
                            <div class="block-2">
                                <span class="etiqueta">Maestria:</span>
                                <span class="info-etiqueta"><%- filas[0].Maestria %> </span>
                            </div>
                        </div>
                        <div class="blocks-row">
                            <div class="block-2">
                                <span class="etiqueta">Medio de Transporte:</span>
                                <%
                                var medio_trans = filas[0].Medio_de_transporte;
                                if (medio_trans === 1) {%>
                                    <span class="info-etiqueta">Vehiculo Propio.</span>
                                <%}else if (medio_trans === 2) {%>
                                    <span class="info-etiqueta">Motocicleta.</span>
                                <%}else if (medio_trans === 3) {%>
                                    <span class="info-etiqueta">Transporte Publico.</span>
                                <%}else if (medio_trans === 4) {%>
                                    <span class="info-etiqueta">Transporte Privado.</span>
                                <%}else if (medio_trans === 5) {%>
                                    <span class="info-etiqueta">Ninguno.</span>
                                <%}else{%>
                                    <span class="info-etiqueta">No Especifico.</span>
                                <%}%>
                            </div>
                            <div class="block-2">
                                <span class="etiqueta">Aplica a Beca:</span>
                                <span class="info-etiqueta"><%- filas[0].Nombre_Rubro %> </span>
                            </div>
                        </div>
                        <% if (filas[0].Serevicio_social) {%>
                            <div class="blocks-row">
                                <div class="block-1">
                                    <span class="etiqueta">Cantidad de Horas Sociales:</span>
                                    <%
                                    var horas_sociales = filas[0].Horas_sociales_realizadas;
                                    if(horas_sociales ===1) { %>
                                        <span class="info-etiqueta">Menos de 100 horas</span>
                                    <% }else  if(horas_sociales ===2){ %>
                                        <span class="info-etiqueta">Mas de 100 horas</span>
                                    <% }else  if(horas_sociales ===3){ %>
                                        <span class="info-etiqueta">Menos de 200 horas</span>
                                    <% }else  if(horas_sociales ===4){ %>
                                        <span class="info-etiqueta">Mas de 200 horas</span>
                                    <% }else  if(horas_sociales ===5){ %>
                                        <span class="info-etiqueta">Menos de 300 horas</span>
                                    <% }else  if(horas_sociales ===6){ %>
                                        <span class="info-etiqueta">Mas de 300 horas</span>
                                    <% }else  if(horas_sociales ===7){ %>
                                        <span class="info-etiqueta">Menos de 400 horas</span>
                                    <% }else  if(horas_sociales ===8){ %>
                                        <span class="info-etiqueta">Mas de 400 horas</span>
                                    <% }else  if(horas_sociales ===9){ %>
                                        <span class="info-etiqueta">Menos de 500 horas</span>
                                    <% }else  if(horas_sociales ===10){ %>
                                        <span class="info-etiqueta">Mas de 500 horas</span>
                                    <% }else{ %>
                                        <span class="info-etiqueta">No Especifico.</span>
                                    <% } %>
