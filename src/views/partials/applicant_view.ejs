                        <div class="blocks-row">
                            <div class="block-1">
                                <span class="etiqueta">Direccion:</span>
                                <span class="info-etiqueta"><%- filas[0].Direccion %> </span>
                            </div>
                        </div>
                        <div class="blocks-row">
                            <div class="block-3">
                                <span class="etiqueta">Telefono:</span>
                                <span class="info-etiqueta"><%- filas[0].Telefono %> </span>
                            </div>
                            <div class="block-3">
                                <span class="etiqueta">Celular:</span>
                                <span class="info-etiqueta"><%- filas[0].Telefono %> </span>
